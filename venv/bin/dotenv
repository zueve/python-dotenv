#!/Users/theskumar/work/theskumar/python-dotenv/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'python-dotenv','console_scripts','dotenv'
__requires__ = 'python-dotenv'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('python-dotenv', 'console_scripts', 'dotenv')()
    )
